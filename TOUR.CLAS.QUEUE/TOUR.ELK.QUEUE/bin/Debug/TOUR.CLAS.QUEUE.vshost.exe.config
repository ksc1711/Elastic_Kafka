<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>

    <!-- Y:테스트 / N:실제적용  (테스트 소스일 경우 날짜가 2016-09-22로 고정되어있음) -->
    <add key="TEST" value="Y" />

    <!-- Y:로그이름에 - 제거 / N: - 붙여서 날짜 선정 및 폴더이름 선정-->
    <add key="NOT_HYPHEN" value="N" />
    
    <!--SearchDrive : Log파일 폴더 위치-->
    <add key="SEARCH_DRIVE" value="D:\ComLog" />

    <!--TCP Message -->
    <add key="SEND_OK" value="SendOK" />

    <!--TCP Server 주소 -->
    <add key="SOCKET_IP" value="127.0.0.1" />
    <add key="SOCKET_PORT" value="1900" />
    <add key="MAX_THREAD" value="10" />
    <add key="DISTANCE" value="300" />

    <!--LOG TYPE -->
    <add key="LOG_TYPE" value="TRACE|ERROR|INFO|DEBUG|WARN" />
    <add key="LOG_TYPE_LOWER" value="Trace|Error|Info|Debug|Warn" />

    <!--ElasticSerach -->
    <add key="ELASTIC_HOST" value="http://192.168.28.174:9200/" />
    <add key="ELASTIC_PORT" value="9200" />
    <add key="INDEX_NAME" value="tourloginfo" />


  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="System.Data.SQLite" publicKeyToken="db937bc2d44ff139" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.0.104.0" newVersion="1.0.104.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>